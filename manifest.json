{
  "browser_action": {
    "default_icon": "reload.png"
  },
  "description": "Reload discarded tabs after a session restore to obtain favico",
  "homepage_url": "https://github.com/irvinm/ReloadDiscardedTabs",
  "manifest_version": 2,
  "name": "Reload Discarded Tabs",
  "version": "0.9.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background-rdt.js"]
  },
  "icons": {
    "128": "reload.png"
  },
  "applications": {
    "gecko": {
      "id": "Reload-Discarded-Tabs@irvinm.addons.mozilla.org"
    }
  },
  "web_accessible_resources": ["images/*.png"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}